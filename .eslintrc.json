{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "linebreak-style": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off"
  }
}
